<div class="container">
        <div class="photo-index">
            <% @photos.each do |photo| %>
                <div class="photo-box">
                    <%= link_to photo_path(photo.id) do %>
                        <%= image_tag photo.image.thumb.to_s %>
                    <% end %>
                </div>
            <% end %>
        </div>
    <div class="top_button">
        <%= link_to 'トップページへ戻る', root_path, class: 'btn btn-default', data: {"turbolinks" => false} %>
    </div>
</div>